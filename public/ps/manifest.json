{
  "id": "11223344",
  "name": "UXP Template",
  "version": "1.0.0",
  "main": "index.html",
  "manifestVersion": 4,
  "host": {
    "app": "PS",
    "minVersion": "22.0.0"
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "uxpTemplateController",
      "label": {
        "default": "UXP Template"
      },
      "minimumSize": {
        "width": 300,
        "height": 400
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 230,
        "height": 300
      },
      "preferredFloatingSize": {
        "width": 230,
        "height": 300
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icon/dark-panel.png",
          "scale": [1, 2],
          "theme": ["darkest", "dark", "medium"],
          "species": ["chrome"]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icon/light-panel.png",
          "scale": [1, 2],
          "theme": ["lightest", "light"],
          "species": ["chrome"]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 48,
      "height": 48,
      "path": "icon/plugin.png",
      "scale": [1, 2],
      "theme": ["darkest", "dark", "medium", "lightest", "light", "all"],
      "species": ["pluginList"]
    }
  ]
}
